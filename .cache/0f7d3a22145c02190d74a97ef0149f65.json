eLoading = (status = false,tip = '再等一下') => {\r\n        this.setState({\r\n            tip,\r\n            loading: status\r\n        })\r\n    }\r\n    render () {\r\n        const { children } = this.props;\r\n        const { loading, tip } = this.state;\r\n        return (\r\n            <div className=\"flex-column\" style={{width:'100%',height: '100%'}}>\r\n                <Menu \r\n                    mode=\"horizontal\" \r\n                    style={{fontSize:13.5,backgroundColor:'#000'}}\r\n                    defaultSelectedKeys={[this.matchRouteName]}\r\n                >\r\n                <Menu.Item style={{marginLeft:24,marginRight:30,fontSize:18,textAlign:'center',color:'#fff !important',float:'left'}}>\r\n                    <a href={'/'} className=\"hover-scale logo-text\" style={{color:'#fff2e8'}}>\r\n                        预先网站\r\n                    </a>\r\n                </Menu.Item>\r\n                {\r\n                    navRoutes.map((e,i)=>(\r\n                        <Menu.Item key={e.name}>\r\n                            {\r\n                                getMenuContent({...e})\r\n                            }\r\n                        </Menu.Item>\r\n                    ))\r\n                }\r\n                </Menu>\r\n                <Spin\r\n                    spinning={loading}\r\n                    tip={tip}\r\n                    wrapperClassName='content-spin full'\r\n                >\r\n                    {children}\r\n                </Spin>\r\n            </div>\r\n        )\r\n    }\r\n}"},"lineCount":null}},"hash":"31c2a4c96e912fb029dee382b71f57ef","cacheData":{"env":{}}}